Config {
         font = "xft:Dejavu Sans-6"
       , bgColor  = "#777777"
       , fgColor  = "black"
        , position =  Static {xpos = 0, ypos = 0, width = 1740, height = 20}
       , lowerOnStart = False
       , commands = [ Run Network "wlan0" ["-t", "N(<rx>, <tx>)"] 5
                    , Run Network "eth0"  ["-t", "N(<rx>, <tx>)"] 5
                    , Run Cpu             ["-t", "C(<total>, <nice>, <system>)"] 5
                    , Run Memory          ["-t", "M(<usedratio>%,"] 10
                    , Run Swap            ["-t","<used>)"] 10
                    , Run Date            "%Y-%m-%d %H:%M" "date" 10
                    , Run CoreTemp        ["-t", "T(<core0>Ëš)"] 10
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% }{ <fc=#000000,#55cc55>%cpu%</fc> <fc=#000000,#9999dd>%memory% %swap%</fc> <fc=#000000,#55aaaa>%wlan0%</fc> <fc=#000000,#bbbb66>%coretemp%</fc> <fc=#000000,#000000> </fc>%date%<fc=#000000,#000000> </fc>"
       }

